<!--components/tabbar/index.wxml-->
<van-tabbar active="{{ active }}" bind:change="onChange" active-color="#191919" inactive-color="#999999" fixed>
  <van-tabbar-item icon="notes-o">日程</van-tabbar-item>
  <van-tabbar-item>
    <image
      slot="icon"
      src="{{ icon.normal }}"
      mode="aspectFit"
      style="width: 46px; height: 46px;margin-top:11rpx;"
    />
    <image
      slot="icon-active"
      src="{{ icon.normal }}"
      mode="aspectFit"
      style="width: 46px; height: 46px;margin-top:11rpx;"
    />
  </van-tabbar-item>
  <van-tabbar-item icon="contact">我</van-tabbar-item>
</van-tabbar>
